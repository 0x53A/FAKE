<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <Import Project="$(MSBuildExtensionsPath)\$(MSBuildToolsVersion)\Microsoft.Common.props" />
  <PropertyGroup>
    <VersionPrefix>1.0.0-alpha10</VersionPrefix>
    <TargetFramework>netstandard1.6</TargetFramework>
    <DefineConstants>$(DefineConstants);CORE_CLR;DOTNETCORE;EXPLICIT_DEPENDENCIES</DefineConstants>
    <DelaySign>true</DelaySign>
    <DebugType>pdbonly</DebugType>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <AssemblyName>Fake.Runtime</AssemblyName>
    <OutputType>Library</OutputType>
    <PackageTargetFallback Condition=" '$(TargetFramework)' == 'netstandard1.6' ">$(PackageTargetFallback);portable-net45+win8;dnxcore50</PackageTargetFallback>
  </PropertyGroup>
  
  <PropertyGroup Condition=" '$(TargetFramework)' == 'netstandard1.6' ">
    <DefineConstants>$(DefineConstants);NETSTANDARD1_6;NETSTANDARD1_5</DefineConstants>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DefineConstants>$(DefineConstants);RELEASE</DefineConstants>
  </PropertyGroup>


  <ItemGroup>
    <Compile Include="..\..\..\paket-files\matthid\Yaaf.FSharp.Scripting\src\source\Yaaf.FSharp.Scripting\YaafFSharpScripting.fs" />
    <Compile Include="AssemblyInfo.fs" />
    <Compile Include="LegacyApiHelper.fs" />
    <Compile Include="Environment.fs" />
    <Compile Include="Path.fs" />
    <Compile Include="String.fs" />
    <Compile Include="Trace.fs" />
    <Compile Include="ScriptRunner.fs" />
    <Compile Include="HashGeneration.fs" />
    <Compile Include="CoreCache.fs" />
    <Compile Include="FakeRuntime.fs" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Fake.Core.Context\Fake.Core.Context.fsproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.NET.Sdk">
      <Version>1.0.0-alpha-20161104-2</Version>
      <PrivateAssets>All</PrivateAssets>
    </PackageReference>
    <PackageReference Include="FSharp.NET.Sdk">
      <Version>1.0.0-alpha-000007</Version>
      <PrivateAssets>All</PrivateAssets>
    </PackageReference>
    <PackageReference Include="Paket.Core">
      <Version>3.19.4</Version>
    </PackageReference>
    <PackageReference Include="Mono.Cecil">
      <Version>0.10.0-beta1-v2</Version>
    </PackageReference>    
  </ItemGroup>  

  <ItemGroup Condition=" '$(TargetFramework)' == 'net46' ">
    <PackageReference Include="FSharp.Core">
      <Version>4.0.1.7-alpha</Version>
    </PackageReference>
    <!--<Reference Include="Microsoft.Build.Framework">
      <FromP2P>true</FromP2P>
    </Reference>-->
    <PackageReference Include="FSharp.Compiler.Service">
      <Version>8.0.0</Version>
    </PackageReference>    
    <Reference Include="mscorlib">
      <FromP2P>true</FromP2P>
    </Reference>
    <Reference Include="System">
      <FromP2P>true</FromP2P>
    </Reference>
    <Reference Include="System.Configuration">
      <FromP2P>true</FromP2P>
    </Reference>
    <Reference Include="System.Runtime.Caching">
      <FromP2P>true</FromP2P>
    </Reference>
    <Reference Include="System.ServiceProcess">
      <FromP2P>true</FromP2P>
    </Reference>
    <Reference Include="System.Xml">
      <FromP2P>true</FromP2P>
    </Reference>
    <Reference Include="System.Xml.Linq">
      <FromP2P>true</FromP2P>
    </Reference>
    <Reference Include="System.Core">
      <FromP2P>true</FromP2P>
    </Reference>
  </ItemGroup>

  <ItemGroup Condition=" '$(TargetFramework)' == 'netstandard1.6' ">
    <PackageReference Include="FSharp.Compiler.Service">
      <Version>6.0.3-alpha1</Version>
    </PackageReference>  
    <PackageReference Include="Microsoft.FSharp.Core.netcore">
      <Version>1.0.0-alpha-161023</Version>
    </PackageReference>  
    <PackageReference Include="System.Runtime.Loader">
      <Version>4.3.0</Version>
    </PackageReference>
    <PackageReference Include="NETStandard.Library">
      <Version>1.6.0</Version>
    </PackageReference>
  </ItemGroup>  

  <ItemGroup>
    <DotNetCliToolReference Include="dotnet-compile-fsc">
      <Version>1.0.0-preview2-020000</Version>
    </DotNetCliToolReference>
  </ItemGroup>

  <Import Project="$(MSBuildToolsPath)\Microsoft.Common.targets" />
</Project>